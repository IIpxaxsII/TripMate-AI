# Supabase Configuration (auto-populated by Lovable Cloud)
VITE_SUPABASE_URL=
VITE_SUPABASE_PUBLISHABLE_KEY=
VITE_SUPABASE_PROJECT_ID=

# Currency Settings
VITE_DEFAULT_CURRENCY=INR

# Optional: Force show auth page even with valid session (for testing)
VITE_FORCE_SHOW_AUTH=false

# Server-side secrets (set via supabase secrets set, NOT in .env)
# PEXELS_API_KEY=         # For Pexels image Edge Function
# GEMINI_API_KEY=         # For AI chat and itinerary generation
# OPENAI_API_KEY=         # Alternative to Gemini
# HOTEL_API_KEY=          # Optional: For real hotel API (mock data used if not set)
# EXCHANGE_API_KEY=       # Optional: For paid exchange rate providers (free API used by default)
